		//step 2
		
	    WebDriver driver=new ChromeDriver();
		
	    //To enter URL
	    driver.get("https://www.w3schools.com/js/tryit.asp?filename=tryjs_myfirst");
		
		//wait
	    Thread.sleep(2000);
	
	    //we can pass 4 types of parameter in frame menthod(Framename,Frameid,webElement,Frameindex)
	    //1- to switch to Iframe by Framename
	    //driver.switchTo().frame("iframeResult");
	    
	  //2- to switch to Iframe by Frameid
	    driver.switchTo().frame("iframeResult");
	    
